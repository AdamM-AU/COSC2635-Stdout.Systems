<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_generator"
    android:foregroundTint="#0570B2"
    tools:context=".passwordGeneratorScreen">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/pass_8_btn"
        android:layout_width="351dp"
        android:layout_height="60dp"
        android:backgroundTint="#0570B3"
        android:drawableStart="@drawable/icon_gen"
        android:text="@string/login"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="231dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/pass_12_btn"
        android:layout_width="351dp"
        android:layout_height="60dp"
        android:backgroundTint="#0570B3"
        android:drawableStart="@drawable/icon_gen"
        android:text="@string/login"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="305dp" />
</androidx.constraintlayout.widget.ConstraintLayout>